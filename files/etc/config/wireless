
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option disabled '0'

config wifi-iface
	option network 'wwan'
	option ssid 'Telekom_ICE'
	option encryption 'none'
	option device 'radio0'
	option mode 'sta'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '6'
	option hwmode '11g'
	option path 'platform/ehci-platform/usb1/1-1/1-1:1.0'
	option htmode 'HT20'
	option disabled '0'

config wifi-iface
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'bahn.freifunk.net'
	option encryption 'none'

